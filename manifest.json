{
  "author": "igorlogius",
  "homepage_url": "https://github.com/igorlogius/tabs-media-controller",
  "browser_action": {
    "default_area": "navbar",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "256": "icon.png"
  },
  "manifest_version": 2,
  "name": "Tabs Media Controller",
  "permissions": ["tabs"],
  "version": "1.4.8",
  "web_accessible_resources": ["attach.js"]
}
