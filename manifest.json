{
  "author": "igorlogius",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
	"scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "manifest_version": 2,
  "name": "Tabs Media Controller",
  "permissions": [
    "tabs"
  ],
  "version": "1.0.0"
}
